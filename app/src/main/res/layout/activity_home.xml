<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:fadingEdge="none|horizontal"
    android:visibility="visible"
    android:alpha="1"
        >

    <Button
        android:id="@+id/SearchPlace"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="95dp"
        android:background="@color/wallet_highlighted_text_holo_dark"
        android:fontFamily="sans-serif-condensed"
        android:text="Start Searching"
        android:textColor="@color/cast_expanded_controller_text_color"
        android:textSize="30sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="109dp" />


    <Button
        android:id="@+id/ToLogs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/SearchPlace"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="32dp"
        android:background="@color/wallet_highlighted_text_holo_dark"
        android:fontFamily="sans-serif-condensed"
        android:text="Create Log"
        android:textColor="@color/cast_expanded_controller_text_color"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="109dp" />
</RelativeLayout>